{
  "name": "Palette-builder",
  "version": "1.1.0",
  "description": "A palette building tool, based on the Opium.Fill concept",
  "main": "index.pug",
  "scripts": {
    "start": "parcel src/*.pug --no-cache --no-source-maps -d ./dist/dev/",
    "build": "parcel build src/*.pug -d ./dist/prod/",
    "deploy": "gh-pages -d dist"
  },
  "author": "Irina Illustrova",
	"license": "MIT",
	"svgSpriteOptions": {
		"exclude": [
			"src/icons/gradient.svg"
		]
	},
  "dependencies": {
    "@simonwep/pickr": "^1.7.1",
    "file-saver": "^2.0.2",
    "invert-color": "^2.0.0",
    "milligram": "^1.4.1",
    "parcel-plugin-imagemin": "^4.0.1",
    "parcel-plugin-svg-sprite": "^1.4.1",
    "prismjs": "^1.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-throw-expressions": "^7.10.4",
    "@mdi/svg": "^5.5.55",
    "@prettier/plugin-pug": "^1.4.4",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "gzip-size": "^5.1.1",
    "html-minifier": "^4.0.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "parcel-bundler": "^1.12.4",
    "postcss-extract-to-file": "0.0.2",
    "postcss-modules": "^3.2.0",
    "prettier": "^2.1.1",
    "pug": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.6.0",
    "sass": "^1.27.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0"
